{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_authors}"
    ],
    "contributors": [
        "${mod_credits}"
    ],
    "contact": {
        "sources": "${mod_url}",
        "issues": "${mod_tracker}"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.aetherteam.cumulus.CumulusFabric"
        ],
        "client": [
            "com.aetherteam.cumulus.CumulusFabric"
        ],
        "fabric-datagen": [
            "com.aetherteam.cumulus.data.CumulusDataGenerator"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": "${fabric_loader_version_range}",
        "minecraft": "${fabric_minecraft_version_range}",
        "java": ">=21",
        "fabric-api": "${fabric_version_range}"
    }
}